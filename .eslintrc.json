{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-alert": ["error", "alawys"],
    "no-console": ["error", "alawys"],
    "no-debugger": ["error", "alawys"],
    "no-trailing-spaces": ["error", "alawys"]
  }
}
